{
    "name": "color picker",
    "description": "lets you pick colors from any webpage",
    "version": "1.4",
    "manifest_version": 4,
    "permissions": [
        "clipboardWrite"
    ],
    "action": {
        "default_popup": "mainColor.html",
        "default_icon": "colorWheel.png"
    }
}
